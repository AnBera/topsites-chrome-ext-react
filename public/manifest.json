{
  "name": "Top Sites react",
  "version": "1.2",
  "description": "Shows the top sites in a browser action",
  "permissions": ["topSites"],
  "browser_action": {
    "default_popup": "index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
